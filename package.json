{
  "name": "suggester",
  "version": "0.0.0",
  "main": "./build/src/index.js",
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^3.2.0",
    "@types/ioredis": "^4.28.8",
    "@types/node": "^14.11.2",
    "discord-api-types": "^0.27.1",
    "gts": "^3.1.0",
    "husky": "^7.0.4",
    "nodemon": "^2.0.15",
    "prisma": "^3.9.2",
    "rimraf": "^3.0.2",
    "ts-node": "^10.5.0",
    "tsc-alias": "^1.6.0",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^4.0.3"
  },
  "scripts": {
    "lint": "gts lint",
    "clean": "gts clean && rimraf build && yarn clean:protos",
    "clean:protos": "./scripts/clean_protos",
    "build": "prisma generate && yarn build:protos && tsc && tsc-alias -s",
    "build:clean": "rimraf build && yarn run build",
    "build:protos": "./scripts/make_protos",
    "fmt": "gts fix && yarn prisma format",
    "prepare": "husky install",
    "pretest": "yarn run build",
    "posttest": "yarn run lint",
    "dev": "nodemon"
  },
  "dependencies": {
    "@fluent/bundle": "^0.17.1",
    "@noble/ed25519": "^1.6.0",
    "@prisma/client": "^3.9.2",
    "dotenv": "^16.0.0",
    "fastify": "^3.27.1",
    "ioredis": "^4.28.5",
    "ts-proto": "^1.105.2"
  },
  "engines": {
    "node": ">=v17.5.0"
  },
  "prisma": {
    "seed": "ts-node -r tsconfig-paths/register -r dotenv/config scripts/seed.ts"
  }
}
