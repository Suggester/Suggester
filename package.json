{
  "name": "suggester",
  "devDependencies": {
    "@nx/js": "^16.8.1",
    "@types/node": "^20.6.0",
    "prisma": "^5.3.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "fmt": "pnpm --parallel fmt",
