{
  "extends": "./node_modules/gts/tsconfig-google.json",
  "compilerOptions": {
    "rootDir": ".",
    "outDir": "build",
    "skipLibCheck": true,
    "esModuleInterop": true,
    "baseUrl": ".",
    "paths": {
      "suggester": ["./lib"]
    },
    "typeRoots": ["node_modules/@types", "lib/types/**"]
  },
  "include": ["src/**/*.ts", "lib/**/*.ts", "scripts/**/*.ts"]
}
